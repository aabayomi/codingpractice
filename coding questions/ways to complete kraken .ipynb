   "source": [
    "def numberofMoves(m,n):\n",
    "    if (m == 1 or n == 1):\n",
    "        return 1\n",
    "    else:\n",
    "        return numberofMoves(m-1,n) + numberofMoves(m,n-1)  + numberofMoves(m-1,n+1)\n",
    "    "
   ]
  },
