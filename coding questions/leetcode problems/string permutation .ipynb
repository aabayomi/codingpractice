    "def permute(s):\n",
    "    out = []\n",
    "    if len(s)==1:\n",
    "        out = [s]\n",
    "    else:\n",
    "        for i , let in enumerate(s):\n",
    "            for perm in permute(s[:i] + s[i + 1]):\n",
    "                \n",
    "                out += [let + perm]\n",
    "    return out\n",
    "        \n",
    "        "
   ]
