   "source": [
    "def powerSum(X, N, current = 1):\n",
    "    pw = pow(current, N)\n",
    "#     print pw\n",
    "    if pw > X:\n",
    "        return 0\n",
    "    elif pw == X:\n",
    "        return 1\n",
    "    else:\n",
    "        return powerSum(X, N, current+1) + powerSum(X-pw, N, current+1)\n"
   ]
